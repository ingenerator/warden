{
    "name":         "ingenerator/warden",
    "type":         "kohana-module",
    "description":  "Complete user authentication, authorisation and management module for Kohana3.3",
    "keywords":     ["web", "auth", "acl", "kohana"],
    "homepage":     "http://github.com/ingenerator/warden",
    "license":      "BSD-3-Clause",
    "authors": [
        {
            "name":      "Andrew Coulton",
            "email":     "andrew@ingenerator.com",
            "homepage":  "http://ingenerator.com"
        }
    ],
    "require": {
        "php":                          ">=5.3.3",
        "composer/installers":          "~1.0",
        "kohana/core":                  "dev-ingenerator-master",
        "ingenerator/kohana-doctrine2": "dev-master",
        "ircmaxell/password-compat":    "1.0.*@stable"
    },
    "require-dev": {
        "phpspec/phpspec":              "2.0.*@dev",
        "ingenerator/koharness":        "dev-master"
    },
    "repositories": [
        {"type": "vcs", "url": "https://github.com/ingenerator/kohana-core"}
    ],
    "config": {
        "preferred-install": "dist"
    }
}
